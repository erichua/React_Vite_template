{
    "compilerOptions": {
      "allowUnreachableCode": false,
      "allowUnusedLabels": false,
      "noEmitOnError": true,
      "noImplicitReturns": true,
      "pretty": true,
      "strict": true,
      "skipLibCheck": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "downlevelIteration": true,
      "types": []
    },
    "exclude": ["node_modules"]
  }
  