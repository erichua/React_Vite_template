model item_intro_package {
  application_id              String                        @db.Char(8)
  package_id                  Int                           @db.TinyInt
  size                        String?                       @db.VarChar(20)
  pack1                       Decimal?                      @db.Decimal(9, 4)
  pack2                       Decimal?                      @db.Decimal(9, 4)
  pack3                       Decimal?                      @db.Decimal(9, 4)
  pack4                       Decimal?                      @db.Decimal(9, 4)
  uom_id                      String?                       @db.Char(5)
  gtin                        String?                       @db.Char(14)
  store_selling_pack          Boolean?
  dc_selling_pack             Boolean?
  purchase_pack               Boolean?
  gst                         Boolean?
  create_time                 DateTime?                     @db.DateTime(0)
  create_by                   String?                       @db.Char(8)
  update_time                 DateTime?                     @db.DateTime(0)
  update_by                   String?                       @db.Char(8)
  keyid                       String?                       @db.VarChar(100)
  tax_code                    String?                       @db.VarChar(20)
  item_intro                  item_intro                    @relation(fields: [application_id], references: [application_id], onUpdate: Restrict, map: "item_intro_package_ibfk_1")
  item_intro_province_package item_intro_province_package[]

  @@id([application_id, package_id])
}

