model item_intro_province_package {
  application_id      String              @db.Char(8)
  package_id          Int                 @db.TinyInt
  province_id         String              @db.Char(3)
  dc_purchase_cost    Decimal?            @db.Decimal(9, 4)
  store_purchase_cost Decimal?            @db.Decimal(9, 4)
  retail_price        Decimal?            @db.Decimal(9, 4)
  online_package      Boolean?
  create_time         DateTime?           @db.DateTime(0)
  create_by           String?             @db.Char(8)
  update_time         DateTime?           @db.DateTime(0)
  update_by           String?             @db.Char(8)
  keyid               String?             @db.VarChar(100)
  item_intro_package  item_intro_package  @relation(fields: [application_id, package_id], references: [application_id, package_id], onUpdate: Restrict, map: "item_intro_province_package_ibfk_1")
  item_intro_province item_intro_province @relation(fields: [application_id, province_id], references: [application_id, province_id], onUpdate: Restrict, map: "item_intro_province_package_ibfk_2")

  @@id([application_id, package_id, province_id])
  @@index([application_id, province_id], map: "application_id")
}

